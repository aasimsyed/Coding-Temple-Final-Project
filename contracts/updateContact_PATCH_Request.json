{
  "request": {
    "method": "PATCH",
    "url": "/contacts/{{contactId}}",
    "headers": {
      "Authorization": "Bearer {{token}}",
      "Content-Type": "application/json"
    },
    "body": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        }
      },
      "required": ["firstName"]
    }
  }
}
