{
  "request": {
    "method": "POST",
    "url": "https://thinking-tester-contact-list.herokuapp.com/users/login",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },
        "password": {
          "type": "string"
        }
      },
      "required": ["email", "password"]
    }
  }
}
