{
  "request": {
    "method": "POST",
    "url": "https://thinking-tester-contact-list.herokuapp.com/users",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "password": {
          "type": "string"
        }
      },
      "required": ["firstName", "lastName", "email", "password"]
    }
  }
}
